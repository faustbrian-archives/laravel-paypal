{
    "name": "artisanry/paypal",
    "description": "A PayPal bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-PayPal",
        "Laravel PayPal"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "graham-campbell/manager": "^3.0",
        "paypal/rest-api-sdk-php": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\PayPal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\PayPal\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\PayPal\\PayPalServiceProvider"
            ],
            "aliases": {
                "PayPal": "Artisanry\\PayPal\\Facades\\PayPal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
