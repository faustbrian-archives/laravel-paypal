{
    "name": "faustbrian/laravel-paypal",
    "description": "A PayPal bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-PayPal", "Laravel PayPal", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.*",
        "graham-campbell/manager": "^3.0",
        "paypal/rest-api-sdk-php": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\PayPal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\PayPal\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\PayPal\\PayPalServiceProvider"
            ],
            "aliases": {
                "PayPal": "BrianFaust\\PayPal\\Facades\\PayPal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
